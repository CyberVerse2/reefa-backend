{
  "name": "startease-expressjs-project",
  "version": "1.0.0",
  "description": "Expressjs project generated using startease CLI tool",
  "main": "index.js",
  "scripts": {
    "start -prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/server.js",
    "start": "node build/server.js",
    "build": "tsc",
    "dev": "nodemon",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/src/common/confi/gdb.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./src/common/config/db.ts migration:generate ./src/common/migrations/user",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/common/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/common/config/db.ts migration:revert",
    "schema:sync": "npm run typeorm schema:sync -- -d ./src/common/configs/db.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.6",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "referral-codes": "^3.0.0",
    "reflect-metadata": "^0.2.1",
    "ts-custom-error": "^3.3.1",
    "tsconfig-paths": "^3.14.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.5",
    "@types/compression": "^1.7.5",
    "@types/connect-timeout": "^0.0.37",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/hpp": "^0.2.3",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^2.1.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "_moduleAliases": {
    "@": "./build"
  }
}
